{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "name": "Microsoft Azure Marketplace Application Installer",
  "short_name": "Azure",
  "display": "standalone",
  "scope": "/",
  "start_url": ".",
  "icons": [
      {
          "src": "./Azure.png",
          "type": "image/png",
          "sizes": "48x48"
      },
      {
        "src": "./Azure-512p-maskable.png",
        "type": "image/png",
        "sizes": "512x512"
    }
  ],
  "prefer_related_applications": true,
  "related_applications": [
      {
          "platform": "play",
          "id": "com.microsoft.azure",
          "url": "https://aka.ms/ReferAzureAndroidAppHome"
      },
      {
          "platform": "itunes",
          "url": "https://aka.ms/ReferAzureIOSPortalHome"
      }
  ]
}
